@model Domain.Dto.BaseListResultDto<Domain.Dto.Identificacao.UsuarioListDto>

<form asp-action="Index" method="get">
    <p>
        Search: <input type="text" name="searchString" value="@ViewData["CurrentFilter"]" />
        <input type="submit" value="Search" />
    </p>
</form>

<table>
    <thead>
        <tr>
            <th>
                @Html.ActionLink("Name", "Index", new { sortOrder = ViewData["NameSortParm"], searchString = ViewData["CurrentFilter"] })
            </th>
            <!-- outras colunas -->
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Items)
        {
            <tr>
                <td>@(item.Nome)</td>
                <!-- outras colunas -->
            </tr>
        }
    </tbody>
</table>

<div>
    Page @(Model.Pagina) of @Model.Total
</div>

<nav>
    <ul class="pagination">
        @for (int i = 1; i <= Model.Total; i++)
        {
            <li class="@(i == Model.Pagina ? "active" : "")">
                <a asp-action="Index" asp-route-pageIndex="@i" asp-route-sortOrder="@ViewData["CurrentSort"]" asp-route-searchString="@ViewData["CurrentFilter"]">@i</a>
            </li>
        }
    </ul>
</nav>